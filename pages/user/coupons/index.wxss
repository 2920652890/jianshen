page {
  background-color: #f5f5f5;
}
.coupons-page {
  padding: 0 20rpx;
}
.coupons-page .item {
  color: #fff;
  height: 206rpx;
  background: #d71f1a;
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.05);
  border-radius: 4px;
  position: relative;
  padding: 40rpx 110rpx 40rpx 10rpx;
  display: flex;
  overflow: hidden;
  margin: 0 28rpx;
}
.coupons-page .item .tip {
  font-size: 20rpx;
  position: absolute;
  left: 20rpx;
  top: 16rpx;
}
.coupons-page .item .num {
  width: 206rpx;
  font-size: 108rpx;
  border-right: 1px solid #fff;
  text-align: center;
}
.coupons-page .item .f1 {
  padding: 6rpx 0 0 34rpx;
}
.coupons-page .item .f1 .h1 {
  font-size: 28rpx;
}
.coupons-page .item .f1 .h2 {
  font-size: 24rpx;
  margin-top: 12rpx;
  color: #e6e6e6;
}
.coupons-page .item .f1 .h3 {
  margin-top: 2rpx;
  font-size: 20rpx;
  color: #c9c9c9;
}
.coupons-page .item .use {
  position: absolute;
  top: 0;
  right: 0;
  height: 100%;
  color: #000;
  width: 110rpx;
  background-color: #fff;
  font-size: 28rpx;
  padding: 0 40rpx;
  display: flex;
  align-items: center;
}
