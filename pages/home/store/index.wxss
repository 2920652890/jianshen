.store-page {
  width: 100%;
  height: 100%;
}
.store-page .container {
  padding-bottom: 180rpx;
}
.store-page .banner {
  height: 540rpx;
}
.store-page .pd10 {
  padding: 0 20rpx;
}
.store-page .menu {
  padding: 0 30rpx;
  font-size: 36rpx;
  display: flex;
}
.store-page .menu .item {
  padding: 30rpx 0;
  margin-right: 48rpx;
  color: #b7b7b7;
}
.store-page .menu .active {
  color: #000;
}
.store-page .content {
  height: max-content;
}
.store-page .box {
  background-color: #fff;
  padding: 32rpx 36rpx;
  margin-bottom: 24rpx;
}
.store-page .box .title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
}
.store-page .box .title .span {
  color: #999999;
  font-size: 24rpx;
}
.store-page .msg1 {
  padding: 0 36rpx;
}
.store-page .msg1 .tip-box {
  position: relative;
  padding-top: 32rpx;
}
.store-page .msg1 .tip-box .tip-box-text {
  margin-right: 120rpx;
}
.store-page .msg1 .tip-box .h1 {
  font-size: 28rpx;
  margin-bottom: 13rpx;
}
.store-page .msg1 .tip-box .h2 {
  font-size: 26rpx;
  color: #666666;
  margin-bottom: 18rpx;
}
.store-page .msg1 .tip-box .phone {
  position: absolute;
  right: 0;
  bottom: 2rpx;
  font-size: 24rpx;
  text-align: center;
  color: #999999;
}
.store-page .msg1 .tip-box .phone image {
  width: 52rpx;
  height: 64rpx;
  margin: 0 auto 10rpx auto;
}
.store-page .msg1 .ad {
  margin-top: 14rpx;
  border-top: 1px solid #f5f5f5;
  padding: 18rpx 0 28rpx 0;
  display: flex;
  align-items: center;
}
.store-page .msg1 .ad .i1 {
  width: 20rpx;
  height: 24rpx;
  margin-right: 20rpx;
}
.store-page .msg1 .ad .f1 {
  color: #999999;
}
.store-page .msg1 .ad .iconfont {
  margin-left: 20rpx;
  color: #979797;
}
.store-page .msg2 {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #666666;
}
.store-page .msg2 .f1 {
  margin-left: 30rpx;
  display: flex;
  flex-direction: row-reverse;
}
.store-page .msg3 {
  padding: 32rpx 0;
}
.store-page .msg3 .title {
  padding: 0 36rpx;
}
.store-page .msg3 .utils {
  margin-top: 20rpx;
  overflow: hidden;
  white-space: nowrap;
}
.store-page .msg3 .utils .item {
  display: inline-block;
  margin-left: 36rpx;
  width: 256rpx;
  height: 310rpx;
  text-align: center;
  font-size: 24rpx;
}
.store-page .msg3 .utils .item:last-child {
  margin-right: 36rpx;
}
.store-page .msg3 .utils .item image {
  width: 256rpx;
  height: 256rpx;
  margin-bottom: 20rpx;
  padding: 30rpx;
}
.store-page .msg4 {
  padding: 32rpx 36rpx 0 36rpx;
}
.store-page .msg4 .service-list {
  margin-top: 36rpx;
  display: flex;
  flex-wrap: wrap;
}
.store-page .msg4 .service-list .item {
  width: 25%;
  margin-bottom: 30rpx;
  height: 94rpx;
  position: relative;
}
.store-page .msg4 .service-list .item image {
  margin: 0 auto;
  width: inherit;
  height: initial;
}
.store-page .msg4 .service-list .item .p {
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 22rpx;
  color: #999999;
  width: 100%;
  text-align: center;
}
.store-page .msg5 .list {
  margin-top: 28rpx;
  font-size: 24rpx;
  color: #666666;
}
.footer {
  position: fixed;
  left: 20rpx;
  right: 20rpx;
  bottom: 20rpx;
}
